<%= simple_form_for [:admin, @feedback] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description, label: t(:feedback_description) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, t(:update) %> <%= link_to t(:back), admin_feedbacks_path %>
  </div>
<% end %>
