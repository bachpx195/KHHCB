<p id="notice"><%= notice %></p>

<h1>User Adits</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Cource</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_adits.each do |user_adit| %>
      <tr>
        <td><%= user_adit.user %></td>
        <td><%= user_adit.cource %></td>
        <td><%= user_adit.action_type %></td>
        <td><%= link_to 'Show', user_adit %></td>
        <td><%= link_to 'Edit', edit_user_adit_path(user_adit) %></td>
        <td><%= link_to 'Destroy', user_adit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Adit', new_user_adit_path %>
